{
  "manifest_version": 2,
  "name": "My Dict ext",
  "version": "1.0",
  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js":["content.js"]
    }
  ],
  "background" :{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon":"s.png",
    "default_popup":"popup.html",
    "default_title":"word lookup"
  }
}
